{
    "name": "polifonic/twig-validator-extension",
    "description": "A Twig extension to validate objects inside a Twig template",
    "keywords": ["twig", "validator", "symfony"],
    "homepage": "https://github.com/polifonic/twig-validator-extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Pichon / Polifonic",
            "email": "op@polifonic.io",
            "homepage": "http://www.polifonic.io"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "twig/twig": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8||~5.4",
        "symfony/validator": "^2.5||^3.0"
    },
    "autoload": {
        "psr-4": { "Polifonic\\Twig\\Extension\\Validator\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
